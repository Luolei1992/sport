.team .teamTip {
  text-align: center;
  margin: 160rpx 0;
}
.team .teamPub {
  width: 50%;
  background-color: #2d8cf0;
  margin-top: 80rpx;
}
.team .teamLis {
  padding: 30rpx;
  box-sizing: border-box;
}
.team .teamLis .teamPart {
  border: 1px solid #cccccc;
  padding: 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.team .teamLis .teamPartL {
  width: 40%;
  background-color: #ccc;
  float: left;
}
.team .teamLis .teamPartL image {
  width: 100%;
}
.team .teamLis .teamPartR {
  display: inline-block;
  width: 60%;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.team .teamLis .teamName {
  padding-bottom: 20rpx;
  display: block;
  font-size: 32rpx;
}
.team .teamLis .teamPartT {
  font-size: 28rpx;
}
.teamCreat {
  padding: 30rpx;
}
.teamCreat .teamMsg {
  margin: 40rpx 0;
}
.teamCreat .teamName {
  margin-bottom: 20rpx;
  display: inline-block;
}
.teamCreat .teamIpt {
  border: 1px solid #ccc;
}
.teamCreat .teamTxt {
  border: 1px solid #ccc;
  width: 100%;
}
.teamCreat .teamPart {
  width: 50%;
  padding: 0 20rpx;
  display: inline-block;
  box-sizing: border-box;
}
.teamCreat .teamType {
  padding-left: 0;
}
.teamCreat .teamPoint {
  padding-right: 0;
}
.teamCreat .teamMember {
  display: table;
  border-collapse: collapse;
}
.teamCreat .teamMember td {
  border: 1px solid #ccc;
}
